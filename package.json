{
  "name": "dit-vue",
  "description": "DIT front-end components",
  "version": "1.3.1",
  "author": "Tyom Semonov <inspiro@gmail.com>",
  "private": true,
  "main": "./dist/components.build.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "examples:build": "npm run build && run-p examples:build:*",
    "examples:build:webpack": "webpack --config examples/webpack/webpack.config.js",
    "examples:build:components": "webpack --config examples/components/webpack.config.js",
    "examples:run:simple": "cp -rf dist examples/simple && cd examples/simple && python -m SimpleHTTPServer",
    "examples:run:webpack": "cd examples/webpack && webpack-dev-server --open",
    "examples:run:components": "cd examples/components && webpack-dev-server --open",
    "deploy": "npm run build && surge --project ./dist --domain envious-rat.surge.sh",
    "postinstall": "cross-env NODE_ENV=development webpack --progress --hide-modules"
  },
  "dependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-latest": "^6.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "highlight.js": "^9.11.0",
    "html-webpack-plugin": "^2.28.0",
    "include-media": "^1.4.9",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "postcss-cssnext": "^2.10.0",
    "postcss-loader": "^2.0.5",
    "pretty": "^2.0.0",
    "sass-loader": "^6.0.5",
    "url-loader": "^0.5.8",
    "vue": "^2.2.1",
    "vue-loader": "^11.3.4",
    "vue-router": "^2.5.3",
    "vue-svg-loader": "^0.1.2",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.2.0"
  },
  "devDependencies": {
    "surge": "^0.19.0",
    "webpack-dev-server": "^2.2.0"
  }
}
